{
  "threshold": 2.5,
  "reporters": ["console", "html"],
  "ignore": [
    "**/node_modules/**",
    "**/*.d.ts",
    "**/src/generated/**",
    "**/.next/**",
    "**/dist/**",
    "**/build/**"
  ],
  "skipComments": true,
  "format": ["typescript", "javascript", "tsx"],
  "minTokens": 45,
  "minLines": 5,
  "mode": "mild",
  "output": "./reports/jscpd"
}
