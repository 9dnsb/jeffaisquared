{
  "name": "jeffaisquared",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env.development -- next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "postinstall": "prisma generate",
    "lint": "eslint",
    "test": "dotenv -e .env.development -- vitest",
    "test:ui": "dotenv -e .env.development -- vitest --ui",
    "test:run": "dotenv -e .env.development -- vitest run",
    "check": "npx tsc --noEmit && npm run lint && npx jscpd src && npm run test:run -- --coverage && set PRISMA_USER_CONSENT_FOR_DANGEROUS_AI_ACTION=yes && npx playwright test && npm run build",
    "db:migrate:dev": "dotenv -e .env.development -- npx prisma migrate dev",
    "db:migrate:prod": "dotenv -e .env.production -- npx prisma migrate deploy",
    "db:migrate:reset:prod": "dotenv -e .env.production -- npx prisma migrate reset --force --skip-seed",
    "db:status": "dotenv -e .env.development -- npx prisma migrate status",
    "db:push": "dotenv -e .env.development -- npx prisma db push",
    "db:push:prod": "dotenv -e .env.production -- npx prisma db push",
    "db:generate": "dotenv -e .env.development -- npx prisma generate",
    "db:generate:prod": "dotenv -e .env.production -- npx prisma generate",
    "db:seed": "timeout 300 dotenv -e .env.development -- npx prisma db seed",
    "db:seed:api": "timeout 86400 dotenv -e .env.development -- npx cross-env USE_SQUARE_API=true npx prisma db seed",
    "db:seed:prod": "dotenv -e .env.production -- npx prisma db seed",
    "db:reset": "dotenv -e .env.development -- npx prisma migrate reset --force",
    "db:reset:prod": "dotenv -e .env.production -- npx prisma migrate reset --force",
    "db:studio": "dotenv -e .env.development -- npx prisma studio",
    "db:studio:prod": "dotenv -e .env.production -- npx prisma studio",
    "db:setup-triggers": "dotenv -e .env.development -- ts-node --compiler-options {\\\"module\\\":\\\"commonjs\\\"} scripts/setup-triggers.ts",
    "db:setup-triggers:prod": "dotenv -e .env.production -- ts-node --compiler-options {\\\"module\\\":\\\"commonjs\\\"} scripts/setup-triggers.ts",
    "db:debug-triggers": "dotenv -e .env.development -- ts-node --compiler-options {\\\"module\\\":\\\"commonjs\\\"} scripts/debug-trigger.ts",
    "db:fix-triggers": "dotenv -e .env.development -- ts-node --compiler-options {\\\"module\\\":\\\"commonjs\\\"} scripts/fix-trigger.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.17.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.75.0",
    "jscpd": "^4.0.5",
    "next": "15.5.4",
    "openai": "^6.2.0",
    "pg": "^8.16.3",
    "prisma": "^6.17.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-markdown": "^10.1.0",
    "rehype-katex": "^7.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.56.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.7.1",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.37.0",
    "eslint-config-next": "15.5.4",
    "eslint-plugin-sonarjs": "^3.0.5",
    "jsdom": "^27.0.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
